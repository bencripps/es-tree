{
  "name": "es-tree",
  "version": "1.0.0",
  "description": "JS2015 Tree Component",
  "jshintConfig": {
    "predef": [],
    "node": "true",
    "jasmine": "true"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^9.0.3",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.4",
    "karma": "~0.12.31",
    "karma-browserify": "~3.0.2",
    "karma-jasmine": "~0.3.5",
    "karma-phantomjs-launcher": "~0.1.4",
    "watchify": "latest"
  },
  "dependencies": {},
  "scripts": {
    "build-js": "browserify src/js/components/tree.js -t babelify --outfile dist/js/estree.js",
    "watch-js": "watchify src/js/*.js -t babelify --outfile dist/js/estree.js",
    "build-styles": "sass src/sass/main.scss dist/css/estree.css",
    "watch-styles": "sass --watch src/sass/main.scss:dist/css/estree.css",
    "test": "karma start"
  },
  "main": "dist/js/estree.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bencripps/estree.git"
  },
  "keywords": [
    "tree",
    "js2015",
    "navigation",
    "es6"
  ],
  "author": "Benjamin Cripps",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bencripps/estree/issues"
  },
  "homepage": "https://github.com/bencripps/estree#readme"
}
